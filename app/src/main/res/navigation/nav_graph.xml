<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.room.pavelfedor.navigationcomponetntexampe.FirstFragment"
        android:label="FirstFragment"
        tools:layout="@layout/firts_fragment">

        <action
            android:id="@+id/actionToSecondFragment"
            app:destination="@+id/secondFragment" />

    </fragment>

    <fragment
        android:id="@+id/secondFragment"
        android:name="com.room.pavelfedor.navigationcomponetntexampe.SecondFragment"
        android:label="SecondFragmen"
        android:layout="@layout/second_fragment">

        <action
            android:id="@+id/actionToFirstFragment"
            app:destination="@+id/firstFragment" />

    </fragment>

</navigation>
